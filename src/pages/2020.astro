---
import golden_gate from '@/assets/2020/golden-gate.jpg'
import muir_beach_overlook from '@/assets/2020/muir-beach-overlook.jpg'
import oceano_dunes from '@/assets/2020/oceano-dunes.jpg'
import molera_beach from '@/assets/2020/molera-beach.jpg'
import pismo_beach from '@/assets/2020/pismo-beach.jpg'
import venice_beach from '@/assets/2020/venice-beach.jpg'
import hollywood from '@/assets/2020/hollywood.jpg'
import track_and_tower from '@/assets/2020/track-and-tower.jpg'
import centennial_ridges from '@/assets/2020/centennial-ridges.jpg'
import killarney_lake from '@/assets/2020/killarney-lake.jpg'
import duck from '@/assets/2020/duck.jpg'
import lake_of_two_rivers from '@/assets/2020/lake-of-two-rivers.jpg'
import osa_lake from '@/assets/2020/osa-lake.jpg'
import cheltenham_badlands from '@/assets/2020/cheltenham-badlands.jpg'
import mono_cliffs from '@/assets/2020/mono-cliffs.jpg'

import Layout from '../layouts/Layout.astro';
import GalleryItem from '../components/GalleryItem.astro';
---

<Layout>
  <GalleryItem source={golden_gate} title="Golden Gate Bridge" description="The Grumpy Old Troll wouldn't be so grumpy if he lived under this bridge" />
  <GalleryItem source={muir_beach_overlook} title="Muir Overlook" description="The coasts of California feel like the edge of the world" />
  <GalleryItem source={oceano_dunes} title="Oceano Dunes" description="Speed is the name of the game" />
  <GalleryItem source={molera_beach} title="Andrew Molera" description="Steeper than it looks" />
  <GalleryItem source={pismo_beach} title="Pismo Beach" description="Lovely evening to gallop along the shores" />
  <GalleryItem source={venice_beach} title="Los Angeles" description="Dreamy landscapes inspire beautiful architecture" />
  <GalleryItem source={hollywood} title="Hollywood" description="A squad of Osprey aircrafts flew overhead as we were leaving" />
  <GalleryItem source={track_and_tower} title="Track and Tower" description="A golden hue so brilliant it's as if the trees turned into fire" />
  <GalleryItem source={centennial_ridges} title="Centennial Ridges" description="Not pictured: swarms upon swarms of black flies" />
  <GalleryItem source={killarney_lake} title="Killarney Lake" description="Heaven is reaching the end of a portage" />
  <GalleryItem source={duck} title="Algonquin" description="Mr.Duck taking a sunset stroll along the beach" />
  <GalleryItem source={lake_of_two_rivers} title="Lake of Two Rivers" description="I would never take a car on a joyride, but a canoe..." />
  <GalleryItem source={osa_lake} title="O.S.A. Lake" description="Survived my first night in the backcountry and we slept on our own island" />
  <GalleryItem source={cheltenham_badlands} title="Cheltenham Badlands" description="Usually I like being outdoors, places like this are the exception" />
  <GalleryItem source={mono_cliffs} title="Mono Cliffs" description="Every hike I fall in love with the Bruce Trail all over again" />
</Layout>
