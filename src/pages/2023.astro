---
import moraine_lake from '@/assets/2023/moraine-lake.jpg'
import marathon from '@/assets/2023/marathon.jpg'
import dyers_bay from '@/assets/2023/dyers-bay.jpg'
import bow_lake from '@/assets/2023/bow-lake.jpg'
import silent_lake from '@/assets/2023/silent-lake.jpg'
import jackson_cove from '@/assets/2023/jackson-cove.jpg'
import peyto_lake from '@/assets/2023/peyto-lake.jpg'
import indian_head_cove from '@/assets/2023/indian-head-cove.jpg'
import hattie_cove from '@/assets/2023/hattie-cove.jpg'
import pinery from '@/assets/2023/pinery.jpg'
import wasaga_beach from '@/assets/2023/wasaga-beach.jpg'
import singhampton_caves from '@/assets/2023/singhampton-caves.jpg'
import nottawasaga_bluffs from '@/assets/2023/nottawasaga-bluffs.jpg'
import roy_thomson_choir_loft from '@/assets/2023/roy-thomson-choir-loft.jpg'
import cherry_blossom from '@/assets/2023/cherry-blossom.jpg'
import austin_bruce_trail from '@/assets/2023/austin-bruce-trail.jpg'
import iroquoia_heights from '@/assets/2023/iroquoia-heights.jpg'
import dundas_valley from '@/assets/2023/dundas-valley.jpg'

import Layout from '../layouts/Layout.astro';
import GalleryItem from '../components/GalleryItem.astro';
---

<Layout gallery="2023">
  <GalleryItem source={moraine_lake} title="Moraine Lake" description="Windows 7 but in real life" />
  <GalleryItem source={marathon} title="Marathon" description="A bench is a temporary respite from the common suffering of a trail runner" /> 
  <GalleryItem source={dyers_bay} title="Dyers Bay" description="We were treated to amazing views the entire way" />
  <GalleryItem source={bow_lake} title="Bow Lake" description="Barefoot training should be started as young as possible" />
  <GalleryItem source={silent_lake} title="Silent Lake" description="A lone pine tree makes me feel some sort of way" />
  <GalleryItem source={jackson_cove} title="Jackson Cove" description="Cloudy days make the forest greens glow" />
  <GalleryItem source={peyto_lake} title="Peyto Lake" description="The deep blue of the glacial lake is something that must be seen with ones eyes" />
  <GalleryItem source={indian_head_cove} title="Indian Head Cove" description="I love seeing natural gradients" />
  <GalleryItem source={hattie_cove} title="Hattie Cove" description="The moodiness of the clouds does well to complement the aesthetic of Lake Superior" />
  <GalleryItem source={pinery} title="Pinery" description="We saw lots of turtles and fishies!" />
  <GalleryItem source={wasaga_beach} title="Wasaga Beach" description="We hiked 25km barefoot to celebrate my 25th birthday" />
  <GalleryItem source={singhampton_caves} title="Singhampton Caves" description="Unintended camoflague at its finest" />
  <GalleryItem source={nottawasaga_bluffs} title="Nottawasaga Bluffs" description="Traversing an icy crevice after forgetting his spikes at home" />
  <GalleryItem source={roy_thomson_choir_loft} title="Roy Thompson Hall" description="Rite of Spring and Gerschwin Piano Concerto in F" />
  <GalleryItem source={cherry_blossom} title="Colpoy Bay" description="Spring" />
  <GalleryItem source={austin_bruce_trail} title="White Bluff" description="Summer" />
  <GalleryItem source={iroquoia_heights} title="Iroquoia Heights" description="Autumn" />
  <GalleryItem source={dundas_valley} title="Dundas Valley" description="Winter" />
</Layout>
